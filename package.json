        "sass-loader": "^8.0.0",
        "vue": "^3.5.13",
        "vue-loader": "^17.4.2",
        "vue-template-compiler": "^2.7.16"
    },
    "overrides": {
        "postcss": "^8.4.31"
    },
    "dependencies": {
        "chart.js": "^4.4.8",
        "vue-chartjs": "^5.3.2"
    }
